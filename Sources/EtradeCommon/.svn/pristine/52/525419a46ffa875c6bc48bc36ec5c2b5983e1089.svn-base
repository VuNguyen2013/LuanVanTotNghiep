#region Using directives

using System;

#endregion

namespace ETradeFinance.Entities
{

    public class BuyRightInfo
    {
        /// <summary>
        /// Gets or sets the sub Account Id
        /// </summary>
        /// <value>The sub Account Id.</value>
        public String SubAccountId { get; set; }

        /// <summary>
        /// Gets or sets the sec symbol.
        /// </summary>
        /// <value>The sec symbol.</value>
        public String SecSymbol { get; set; }

        /// <summary>
        /// Gets or sets the market.
        /// </summary>
        /// <value>The market.</value>
        public String Market { get; set; }

        /// <summary>
        /// Gets or sets the price.
        /// </summary>
        /// <value>The price.</value>
        public decimal Price { get; set; }

        /// <summary>
        /// Gets or sets the volume.
        /// </summary>
        /// <value>The volume.</value>
        public long Volume { get; set; }

        /// <summary>
        /// Gets or sets the registered vol.
        /// </summary>
        /// <value>The registered vol.</value>
        public long RegisteredVol { get; set; }

        /// <summary>
        /// Gets or sets the available vol.
        /// </summary>
        /// <value>The available vol.</value>
        public long AvailableVol { get; set; }

    }
	
}
