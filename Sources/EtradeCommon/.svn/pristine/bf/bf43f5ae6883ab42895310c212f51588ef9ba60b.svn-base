namespace ETradeCore.Entities
{
    public class OrderHistory
    {
        /// <summary>
        /// Gets or sets the symbol.
        /// </summary>
        /// <value>The symbol.</value>
        public System.String Symbol { get; set; }

        /// <summary>
        /// Gets or sets the account no.
        /// </summary>
        /// <value>The account no.</value>
        public System.String AccountNo { get; set; }

        /// <summary>
        /// Gets or sets the order no.
        /// </summary>
        /// <value>The order no.</value>
        public System.Decimal OrderNo { get; set; }

        /// <summary>
        /// Gets or sets the side.
        /// </summary>
        /// <value>The side.</value>
        public System.String Side { get; set; }

        /// <summary>
        /// Gets or sets the volume.
        /// </summary>
        /// <value>The volume.</value>
        public System.Decimal Volume { get; set; }

        /// <summary>
        /// Gets or sets the price.
        /// </summary>
        /// <value>The price.</value>
        public System.Decimal Price { get; set; }

        /// <summary>
        /// Gets or sets the condition.
        /// </summary>
        /// <value>The condition.</value>
        public System.String Condition { get; set; }

        /// <summary>
        /// Gets or sets the condition price.
        /// </summary>
        /// <value>The condition price.</value>
        public System.String ConditionPrice { get; set; }

        /// <summary>
        /// Gets or sets the enter id.
        /// </summary>
        /// <value>The enter id.</value>
        public System.String EnterId { get; set; }

        /// <summary>
        /// Gets or sets the type of the order.
        /// </summary>
        /// <value>The type of the order.</value>
        public System.String OrderType { get; set; }

        /// <summary>
        /// Gets or sets the order date.
        /// </summary>
        /// <value>The order date.</value>
        public System.String OrderDate { get; set; }

        /// <summary>
        /// Gets or sets the order time.
        /// </summary>
        /// <value>The order time.</value>
        public System.String OrderTime { get; set; }

        /// <summary>
        /// Gets or sets the order status.
        /// </summary>
        /// <value>The order status.</value>
        public System.String OrderStatus { get; set; }

        /// <summary>
        /// Gets or sets the match volume.
        /// </summary>
        /// <value>The match volume.</value>
        public System.Decimal MatchVolume { get; set; }

        /// <summary>
        /// Gets or sets the cancel time.
        /// </summary>
        /// <value>The cancel time.</value>
        public System.String CancelTime { get; set; }

        /// <summary>
        /// Gets or sets the cancel volume.
        /// </summary>
        /// <value>The cancel volume.</value>
        public System.Decimal CancelVolume { get; set; }

        /// <summary>
        /// Gets or sets the type of the service.
        /// </summary>
        /// <value>The type of the service.</value>
        public System.String ServiceType { get; set; }

        /// <summary>
        /// Gets or sets the ord seq no.
        /// </summary>
        /// <value>The ord seq no.</value>
        public System.Decimal OrderSeqNo { get; set; }
    }
}