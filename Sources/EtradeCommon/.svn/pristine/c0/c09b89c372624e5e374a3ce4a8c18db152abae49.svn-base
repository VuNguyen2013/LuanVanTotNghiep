namespace ETradeCore.Entities
{
    public class XD
    {
        /// <summary>
        /// Gets or sets the symbol.
        /// </summary>
        /// <value>The symbol.</value>
        public System.String Symbol { get; set; }

        /// <summary>
        /// Gets or sets the avai volume.
        /// </summary>
        /// <value>The avai volume.</value>
        public System.Decimal AvaiVolume { get; set; }

        /// <summary>
        /// Gets or sets the pay date.
        /// </summary>
        /// <value>The pay date.</value>
        public System.DateTime PayDate { get; set; }

        /// <summary>
        /// Gets or sets the close date.
        /// </summary>
        /// <value>The close date.</value>
        public System.DateTime CloseDate { get; set; }

        /// <summary>
        /// Gets or sets the pay rate.
        /// </summary>
        /// <value>The pay rate.</value>
        public System.Decimal PayRate { get; set; }

        /// <summary>
        /// Gets or sets the amount.
        /// </summary>
        /// <value>The amount.</value>
        public System.Decimal Amount { get; set; }

        /// <summary>
        /// Gets or sets the amount after VAT.
        /// </summary>
        /// <value>The amount after VAT.</value>
        public System.Decimal AmountAfterVAT { get; set; }

        /// <summary>
        /// Gets or sets the remark.
        /// </summary>
        /// <value>The remark.</value>
        public System.String Remark { get; set; }

        /// <summary>
        /// Gets or sets the VAT.
        /// </summary>
        /// <value>The VAT.</value>
        public System.Decimal VAT { get; set; }
    }
}